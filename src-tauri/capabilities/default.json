{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "https://**" },
        { "url": "http://localhost:11434/**" },
        { "url": "http://localhost:18800/**" }
      ]
    },
    "sql:default",
    "sql:allow-execute"
  ]
}
